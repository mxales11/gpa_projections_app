<!DOCTYPE html>
<html>
<head>
   <title> <%= full_title(yield(:title)) %></title>
  <%= javascript_include_tag "application" %>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= csrf_meta_tags %>
  <%= render 'layouts/shim' %>
</head>
<body>
	
	
		<div id="frameHeader" class="frame">
			<div id="header">
				<%= render 'layouts/logo_container' %>
			</div>
		</div>		
	
		<div id="frameMain" class="frame">
			<% if flash[:error] %>
				<div id="error_explanation">
					  <p class='error'><%=h flash[:error] %></p>
				</div>
			<% end %>
			<% if flash[:alert] %>
				<div id="error_explanation">
					  <p class='error'><%=h flash[:alert] %></p>
				</div>
			<% end %>
			<% if flash[:notice] %>
				<div id="error_explanation">
					  <p class='error'><%=h flash[:notice] %></p>
				</div>
			<% end %>

			<%= yield %>
		 	
		</div>
		<div id="frameBottom" class="frame">
		<div id="navBottom"><div id="navBottom1">
			<div id="buttonBottom">
			<a href="#" class="buttonBottom">about SMU</a>
			<a href="#" class="buttonBottom">contact us</a>
			</div>
		</div></div>
</div>
</body>
</hmtl>
